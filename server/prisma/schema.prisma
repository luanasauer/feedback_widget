// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// config para criar a tabela no banco
//deve-se rodar o comando npx prisma migrate dev
model Feedback {
  id String @id @default(uuid())

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
